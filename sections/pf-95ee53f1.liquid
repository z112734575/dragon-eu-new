function section2_swiper(font_size){
	// console.log('section2_swiper:'+font_size)
	var swiper = new Swiper('#video_box', {
				watchSlidesProgress: true,
				slidesPerView: 'auto',
				centeredSlides: true,
				loop: true,
				loopedSlides: 5,
				// slidesPerView: 4,
				// autoplay: true,
				width:618*font_size*0.01,
				on: {
					progress: function(progress) {
						let cp = progress.toFixed(3)
						// console.log(cp)
						if(cp == '0.333'||cp == '0.733'){
							video_play()
						}else{
							video_pause()
						}
						
						for (i = 0; i < this.slides.length; i++) {
							var slide = this.slides.eq(i);
							var slideProgress = this.slides[i].progress;
							modify = 1;
							if (Math.abs(slideProgress) > 1) {
								modify = (Math.abs(slideProgress) - 1) * 0.3 + 1;
							}
							translate = slideProgress * modify * 260 * font_size * 0.01 + 'px';
							scale = 1 - Math.abs(slideProgress) / 6;
							zIndex = 999 - Math.abs(Math.round(10 * slideProgress));
							slide.transform('translateX(' + translate + ') scale(' + scale + ')');
							slide.css('zIndex', zIndex);
							slide.css('opacity', 1);
							if (Math.abs(slideProgress) > 3) {
								slide.css('opacity', 0);
							}
						}
					},
					
				},
				setTransition: function(transition) {
					for (var i = 0; i < this.slides.length; i++) {
						var slide = this.slides.eq(i)
						slide.transition(transition);
					}

				}
				
			});
	}
