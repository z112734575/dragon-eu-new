			open_cart:function(){//调取打开购物车组件
				var c = _main;
				$.ajax({
					url: "/cart?view=drawer", 
					type: 'get',
					dataType: 'html',
					cache:false,
					success: function (_html) {
						//console.log(123);
						var obj = document.createElement('div');
					        obj.innerHTML = _html;
						var obj_old = document.getElementById('sidebar-cart');
						//var obj_parent = obj_old.parentNode;
						obj_old.replaceChild(obj.querySelector('.Cart'), obj_old.querySelector('.Cart'));
						setTimeout(function(){
							c.trigger_event(document.getElementById('J_open_sidebar_cart'),'click');		    
						},100);
					},
					complete : function(){
						
					}
				});
					
			},
			trigger_event:function(obj,str_event){
				var obj_event = document.createEvent('HTMLEvents');
				obj_event.initEvent(str_event,true,true);
				obj.dispatchEvent(obj_event);
			},
			_img_init:function(i){//轮播图片初始化及切换颜色
				var c = _main
				let black = [
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image1.png?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image2.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image3.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image4.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image5.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image6.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image7.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image8.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_black_image9.webp?v=1651145198',]
				let white = [
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_white_image1.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_white_image2.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_white_image3.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_white_image4.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_white_image5.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_white_image6.webp?v=1651145198',
					'https://cdn.shopifycdn.net/s/files/1/0257/5246/9566/files/slide_white_image7.webp?v=1651145198',]
				
				for(let i in black){
					c.load_image(black[i],()=>{})
				}
				for(let i in white){
					c.load_image(white[i],()=>{})
				}
				if(i===0){
					c._set_img(black)
				}else{
					c._set_img(white)
				}
				$('.color_wrap .color').eq(i).addClass('active').siblings().removeClass('active')
				$('.color_wrap .color').off().on('click',function(){
					let tag = $(this).attr('is-soldout')
					i = $(this).index()
					$(this).addClass('active').siblings().removeClass('active')
					if(i===0){
						c._set_img(black)
						post_color = 'Black'
						post_id = '39760816046142'
						$('#color_txt').html('Color - Black')
						if(tag == '1'){
							$('.buy_wrap .no_quantity').show()
							$('.buy_wrap .btn_wrap').hide()
						}else{
							$('.buy_wrap .no_quantity').hide()
							$('.buy_wrap .btn_wrap').show()
						}
					}else{
						c._set_img(white)
						post_color = 'White'
						post_id = '39720565407806'
						$('#color_txt').html('Color - White')
						if(tag == '1'){
							$('.buy_wrap .no_quantity').show()
							$('.buy_wrap .btn_wrap').hide()
						}else{
							$('.buy_wrap .no_quantity').hide()
							$('.buy_wrap .btn_wrap').show()
						}
					}
				})
			},
			_set_img:function(images){//动态插入轮播图片
				var c = _main
				mySwiper2.removeAllSlides()
				mySwiper.removeAllSlides()
				for(let i in images){
					mySwiper2.addSlide(i,'<li class="swiper-slide"><div class="img_box"><img src="'+images[i]+'" /></div></li>');
					mySwiper.addSlide(i,'<li class="swiper-slide"><div class="img_box"><img src="'+images[i]+'" /></div></li>');
				}
				c._click_fn()
			},
			_img_slide:function(){//轮播
				var c = _main
				var ww = $(window).width()
				mySwiper2 = new Swiper('.small_slide .swiper', {
					spaceBetween: parseInt(ww/100*4.25),
					slidesPerView: 3,
					loopedSlides: 1,
					watchSlidesVisibility: true,
					watchSlidesProgress: true,
					breakpoints: { 
						1025: {
							spaceBetween: parseInt(ww/100*1.65)
						}
					},
					on:{
						slideChange: function () {
							islide = this.realIndex
						},
					},
				});
				mySwiper = new Swiper(' .big_show .swiper', {
					spaceBetween: 20,
					loopedSlides: 1,
					thumbs: {
						swiper: mySwiper2,
					},
					watchSlidesVisibility: true,
					watchSlidesProgress: true,
					on:{
						slideChange: function () {
							islide = this.realIndex
						},
					},
				});
			},
			_click_fn:function(){
				var c = _main
				$('.small_slide .slide_btn').off().on('click',function(){
					if($(this).hasClass('prev')){
						islide = islide == 0 ? 0 : --islide
						mySwiper.slideTo(islide)
					}else{
						islide = islide == $('.small_slide .swiper .swiper-wrapper>li').length - 1 ? $('.small_slide .swiper .swiper-wrapper>li').length - 1 : ++islide
						mySwiper.slideTo(islide)
					}
				})
			},
			_nav_fixed:function(){
				var c = _main
				let navt = $('.nav_con').offset().top
				let pct = $('.product_content ').offset().top - ($(window).height()*0.5)
				$(window).scroll(function(){
					if($(window).scrollTop()>=navt){
						$('.nav_con').addClass('fixed')
					}else{
						$('.nav_con').removeClass('fixed')
					}
					if($(window).scrollTop()>=pct){
						$('.product_content ').addClass('move')
						$('.top_banner').addClass('mask')
					}else{
						$('.product_content ').removeClass('move')
						$('.top_banner').removeClass('mask')
					}
					c._change_nav()
				})
			},
			_tab_action:function(){
				var h
				if($(window).width()<=1024){
					h = $('.mob_nav_fixed').height()
				}else{
					h = $('.nav_con').height()
				}
				$('.tabs_wrap .tab').off('click').on('click',function(){
					setTimeout(function(){
						$(this).addClass('active').siblings().removeClass('active')
					},501)
					let tag = $(this).attr('data-tag')
					let tagt = $(tag).offset().top
					$('body,html').animate({
						scrollTop: parseInt(tagt - h) + 2
					},500);
				})
			},
			_change_nav:function(){
				var c = _main,i,h
				if($(window).width()<=1024){
					h = $('.mob_nav_fixed').height()
					tag = $('.mob_nav_fixed .tabs_wrap')
					img_tag = $('.imagesShow_mob')
				}else{
					h = $('.nav_con').height()
					tag = $('.nav_con .tabs_wrap')
					img_tag = $('.imagesShow')
				}
				if(!c.isMove){
					img_tag.children('.sc_tag').each(function(){
						if($(window).scrollTop() >= parseInt($(this).offset().top - h + 2)){
							i = $(this).attr('data-num')
						}else{
							i = 0
						}
					})
				}
				console.log(i)
				if(i>=0){
					tag.children('.tab').eq(i).addClass('active').siblings().removeClass('active')
				}else{
					tag.children('.tab').removeClass('active')
				}
			},
			
			
			
			_stop:function(e){
				e.stopPropagation()
			},
			video_hide:function(){
				$('body,html').css('overflow','initial')
				$('.cover_video_mask').removeClass('active')
				$('.cover_video_mask .cover_video').attr('src', '');
			},
			video_show:function(){
				var initW = $(window).width()
				var area = []
				if(initW > 1024){
					area = ['910px', '512px']
				}else if(initW > 750 && initW <= 1024){
					var w = initW/1.5
					area = [w+'px', w/2+'px']
				}else{
					var w = initW/1.171875
					area = [w+'px', w/1.78+'px']
				}
				$('.video_container').css({width:area[0],height:area[1]})
				$('.video_button').off('click').on('click',function(){
