		$('.saying_main .add_more').off('click').on('click',function(){
			b = b+2
			c.section3_add(n*(b-2),n*b,n,true)
			if($('.saying_main .saying_inner').children().length==idata.length){
				$('.saying_main .saying_box').removeClass('empty')
				$('.saying_wmask').hide()
				$('.add_more').hide()
				return false
			}
		})
		
	},
	section3_add:function(s,e,n,z){
		var c = _main
		if(e>=idata.length){
			e = idata.length
		}
		var d = e - n
		var list = idata.slice(s,e)
		for(let i in list){
			var html = `<div class="saying_box">
					<img class="con_img" src="`+list[i].img+`" />
					<div class="content">
						<h3 class="txt">Learn More</h3>
						<img class="head" src="`+list[i].head+`" />
						<p class="name">`+list[i].nickname+`</p>
					</div>
				</div>`
			$('.saying_main .saying_inner').append(html)
			$('.saying_inner .saying_box').removeClass('empty')
		}
		for(let j = 0;j<$('.saying_inner .saying_box').length;j++){
			if(j>=d){
				$('.saying_inner .saying_box').eq(j).addClass('empty')
			}
		}
		if(z){
			var scroll = $(window).scrollTop()
			var boxh = $('.saying_inner .saying_box').eq(0).height()
			$('body,html').animate({
			        scrollTop: scroll+boxh
			},500);
			for(let o = 0;o < list.length; o++){
				let q = $('.saying_inner .saying_box').length - list.length
				setTimeout(()=>{
					$('.saying_inner .saying_box').eq(q+o).addClass('show')
				},(o+2)*100)
			}
		}else{
			$('.saying_inner .saying_box').addClass('show')
		}
		c.section3_click()
	},
	section3_click:function(){
		var c = _main
		$('.saying_box').off('click').on('click',function(){
			$('body,html').css('overflow','hidden')
			var i = $(this).index()
			$('.saying_mask').addClass('active')
			var _data = idata
			if(_data[i].nickname) $('.fixed_nickname').html(_data[i].nickname)
			if(_data[i].occupation) $('.fixed_occupation').html(_data[i].occupation)
			if(_data[i].name) $('#fixed_name').html('Name: '+_data[i].name)
			if(_data[i].country) $('#fixed_country').html('Country: '+_data[i].country)
			if(_data[i].head) $('.fixed_head').attr('src',_data[i].head)
			for(let j in _data[i].intrList){
				var html = `<div>`+_data[i].intrList[j]+`</div>`
				$('.saying_fixed .right_main .con_wrap').append(html)
			}
			for(let n in _data[i].bannerList){
				var html = `<li class="swiper-slide">
						<img src="`+_data[i].bannerList[n]+`" />
					</li>`
				$('.saying_fixed .left_swiper .swiper-wrapper').append(html)
			}
			c.fixed_swiper()
		})
	},
	fixed_swiper:function(){
