var top_tip = {
	_init:function(){
		var c = top_tip;
		c.listen_open();
		c.listen_hover();
			
	},
	listen_open:function(){
		var c = top_tip;
		var obj = $('.J_buy_now');
		obj.click(function(e){
			e.stopPropagation();
			var _index = obj.index(this); 
			if(obj.eq(_index).next().css('display')=='block'){
				obj.eq(_index).next().css('display','none');
			}
			else{
				obj.eq(_index).next().css('display','block');
			}
			var obj_2 = obj.eq(_index).next();
			obj_2.find('.bg').css('height',obj_2.find('.buy_now_list').height()+'px');
		});
		$('body').click(function(e){
			e.stopPropagation();
			obj.next().css('display','none');			 
		});
	},
	listen_hover:function(){
		var c = top_tip;
		var obj = $('.J_buy_now_list_box').find('a');
		obj.mouseover(function(){
			var _index = obj.index(this);
			obj.parent().removeClass('on');
			obj.eq(_index).parent().addClass('on');
		}).mouseout(function(){
			obj.parent().removeClass('on');	
		});
	}
}

