<script type="text/javascript">
$(window).resize(function(){
	page_resize();
});

function page_resize(){
	var int_width = $(window).width();	
	if(int_width<=1024){
		$('html').css('fontSize',int_width/768*100+'%');
		return false;
	}
	$('html').css('fontSize',int_width/1920*100+'%');
}
page_resize();

var ww = $(window).width()
var _main = {
	isMove:false,
	_init:function(){
		var c = _main
		c._click()
		c._swiper1('#slider_1')
		c._swiper1('#slider_2')
		c._swiper1('#slider_3')
		c._swiper2('#slider_4')
		c._swiper2('#slider_5')
		c.slide_click()
	},
	_click:function(){
		var c = _main
		//锚点跳转滑动效果  
		$('a[href*=#],area[href*=#]').click(function () {
			$(this).addClass('active').siblings().removeClass('active')
			c.isMove = true
			if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
				var $target = $(this.hash);
				$target = $target.length && $target || $('[name=' + this.hash.slice(1) + ']');
				if ($target.length) {
					var targetOffset = $target.offset().top;
					$('html,body').animate({
						scrollTop: targetOffset
					},500);
					c.isMove = false
					return false;
				}
			}
		});
	},
	_changeNav:function(){
		var c = _main
		var s = $(window).scrollTop()
		var i = 0
		if(!c.isMove){
			$('.title_con').each(function(){
				if(s >= $(this).offset().top - 100){
					i = $(this).attr('data-tag')
				}
			})
		}
		if(i>=0){
			$('.tab_main a').eq(i).addClass('active').siblings().removeClass('active')
		}else{
			$('.tab_main a').removeClass('active')
		}
	},
	_swiper1:function(tag){
		var mySwiper = new Swiper( tag + ' .swiper-container', {
			slidesPerView: 1,
			slidesPerGroup : 1,
			spaceBetween: ww*0.03125,
			navigation: {
				prevEl: tag + ' .slider_btn .prev',
				nextEl: tag + ' .slider_btn .next',
			},
		});
	},
	_swiper2:function(tag){
		var mySwiper = new Swiper( tag + ' .swiper-container', {
			slidesPerView: 1,
			spaceBetween: ww*0.03125/2,
			navigation: {
				prevEl: tag + ' .slider_btn .prev',
				nextEl: tag + ' .slider_btn .next',
			},
			breakpoints: {
				750: {
					slidesPerView: 3,
				}
			},
			loop: true,
		});
	},
	slide_click:function(){
		var initW = $(window).width()
		var area = []
		if(initW > 1024){
			area = ['910px', '512px']
		}else if(initW > 750 && initW <= 1024){
			var w = initW/1.5
			area = [w+'px', w/2+'px']
		}else{
			var w = initW/1.171875
			area = [w+'px', w/1.78+'px']
		}
		$('.video_container').css({width:area[0],height:area[1]})
		$('.charging_img').off('click').on('click',function(){
			$('body,html').css('overflow','hidden')
			let url = $(this).attr('data-video')
			$('.cover_video_mask iframe').attr('src', url);
			$('.cover_video_mask').addClass('active')
		})
	},
	fixed_stop:function(e){
		e.stopPropagation()
	},
	video_hide:function(){
		$('body,html').css('overflow','initial')
		$('.cover_video_mask').removeClass('active')
		$('.cover_video_mask iframe').attr('src', '');
	},
}
_main._init()
$(window).scroll(function(){
	_main._changeNav()
})



</script>
