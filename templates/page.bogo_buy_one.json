    "1eb25428-f93f-4a26-8b10-701fdb9af09e": {
      "type": "custom-html",
      "settings": {
        "code": "<section class=\"details_box\">\n    <h2>Details<\/h2>\n    <ul>\n        <li>The Spring Starter Ugreen BOGO Event takes place 04\/21\/2023 to 04\/30\/2023 PST.<\/li>\n        <li>This event and subsequent discount <b>ONLY applies to products on the event page. The charger and charging cable must be purchased simultaneously<\/b> for the discount to apply.<\/li>\n        <li>Discount from this event <b>cannot<\/b> be used in conjunction with additional coupons\/discounts.<\/li>\n        <li>Final interpretation of rights belongs solely to Ugreen Group, Ltd.<\/li>\n    <\/ul>\n<\/section>\n<script src=\"\/\/at.alicdn.com\/t\/c\/font_3829374_9fbnj23d42i.js\"><\/script>\n<script src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0257\/5246\/9566\/files\/jquery-1.7.2.min.js?v=1624935298\"><\/script>\n<script type=\"text\/javascript\">\n    $(function () {\n        \/\/ 锚点跳转，浏览器URL后缀不带有锚点字样\n        $('.top_tab a').click(function () {\n            if (location.pathname.replace(\/^\\\/\/, '') == this.pathname.replace(\/^\\\/\/, '') && location.hostname == this.hostname) {\n                let $target = $(this.hash);\n                $target = $target.length && $target || $('[name=' + this.hash.slice(1) + ']');\n                if ($target.length) {\n                    let targetOffset = $target.offset().top;\n                    $('html,body').animate({scrollTop: targetOffset}, 500);\n                    return false;\n                }\n            }\n        });\n\n        $(window).on('scroll', debounce(onScroll, 150));\n\n        function debounce(fn, delay) {\n            let timeout;\n            return function () {\n                clearTimeout(timeout)\n                timeout = setTimeout(() => {\n                    fn.apply(this, arguments)\n                }, delay)\n            }\n        }\n        function onScroll() {\n            const scrollDistance = $(window).scrollTop();\n            $('.gan_charger').each(function (i) {\n                if (Math.floor($(this).position().top) - 100 <= scrollDistance) {\n                    $('.top_tab a.active').removeClass('active');\n                    $('.top_tab a').eq(i).addClass('active');\n                } else if (scrollDistance < Math.floor($('.gan_charger').eq(0).position().top)) {\n                    $('.top_tab a.active').removeClass('active');\n                }\n            });\n        }\n        \/\/ 加购按钮 \n        $('.ug-product-buy-btn').text('Add to Cart');\n    })\n<\/script>",
