function bannerTrigger() {
    var bannerWrap = document.querySelector('.banner-wrap')
    gsap.to('.banner-wrap', {
        scrollTrigger: {
            trigger: ".banner-wrap",
            start: "top top",
            // end: "center center",
            end: () => "+=" + bannerWrap.offsetHeight,
            scrub: true,
            pin: true,
            // markers: true
        }
    })
    gsap.to(".banner-video", {
        scrollTrigger: {
            trigger: ".banner-wrap",
            start: "top top",
            // end: "center center",
            end: () => "+=" + bannerWrap.offsetHeight,
            scrub: true,
            // pin: true
