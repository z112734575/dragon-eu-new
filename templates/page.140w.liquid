function progressTrigger() {
    function numberAdd (target, result, timeTotal) {
        let i = 0;
        let time = timeTotal / result
        let timeId = setInterval(() => {
            if(i < result) {
                i++
                target.textContent = Number(target.textContent) + 1
            } else {
                clearInterval(timeId)
            }

        }, time)
    }

    gsap.to('.ug-104w-screen3  .fast-bar', {
        scrollTrigger: {
            trigger: ".ug-104w-screen3 .box",
            start: "top center",
            once: true,
            onEnter: () => numberAdd(getDom('fast-min'), 34, 500)
            // end: "center center",
            // scrub: true,
        },
        width: '100%',
        duration: 0.5,
    })
    gsap.to('.ug-104w-screen3  .normal-bar', {
        scrollTrigger: {
            trigger: ".ug-104w-screen3 .box",
            start: "top center",
            once: true,
            onEnter: () => numberAdd(getDom('normal-min'), 40, 2500)
