    {{ content_for_header }}
{% render 'usf' %}

    {%- render 'head_assets',t_name:t_name,isRTL:isRTL -%}
    <script src="https://cdn-widgetsrepository.yotpo.com/v1/loader/ijX8lsdGgaVTP7Z9LkSOYw" async></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/clipboard@2.0.10/dist/clipboard.min.js"></script>
    {% comment %} iconfont 的 js {% endcomment %}
    <script src="https://cdn.shopify.com/s/files/1/0257/5246/9566/files/iconfont.js?v=1676862777" async></script>
    {{ 'theme-new.css' | asset_url | stylesheet_tag }}
    {{ 'theme-new.js' | asset_url | script_tag }}
    {% comment %}ug-custom{% endcomment %}

  
